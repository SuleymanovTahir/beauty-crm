  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "postinstall": "patch-package || true",
    "i18n:setup": "node scripts/i18n/setup-i18n.js",
    "i18n:extract": "node node_modules/i18next-parser/bin/cli.js --config scripts/i18n/i18next-parser.config.js && node scripts/i18n/cleanup-duplicates.js > /dev/null 2>&1 && node scripts/i18n/fix-nested-keys.js > /dev/null 2>&1 && echo 'âœ… Extraction complete' && npm run i18n:check-only",
    "i18n:check": "node scripts/check-translations.js",
    "i18n:check-only": "node scripts/check-translations.js",
    "i18n:sync": "node scripts/i18n-sync.js",
    "db:i18n:extract": "cd ../backend && venv/bin/python scripts/translations/db_extract.py",
    "db:i18n:translate": "cd ../backend && venv/bin/python scripts/translations/db_translate.py",
    "db:i18n:sync": "cd ../backend && venv/bin/python scripts/translations/db_sync.py",
    "db:i18n:sync-db": "cd ../backend && venv/bin/python scripts/translations/db_sync_to_db.py",
    "db:i18n:sync-frontend": "cd ../backend && venv/bin/python scripts/translations/sync_frontend_locales.py",
    "db:i18n:auto": "npm run db:i18n:extract && npm run db:i18n:translate && npm run db:i18n:sync && npm run db:i18n:sync-db && npm run db:i18n:sync-frontend",
    "i18n:static": "cd ../backend && venv/bin/python scripts/translations/static_translate.py"
  },
  "overrides": {
    "glob": "^10.0.0",
    "rimraf": "^5.0.0"
