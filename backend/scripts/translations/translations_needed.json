    {
      "id": 18,
      "fields": {
        "question_ru": {
          "ru": "Нужна ли подготовка перед процедурой",
          "key": "public_faq.18.question_ru.4a091b3c",
          "en": null,
          "ar": null,
          "es": null,
          "de": null,
          "fr": null,
          "hi": null,
          "kk": null,
          "pt": null
        },
        "answer_ru": {
          "ru": "Да, для некоторых процедур требуется подготовка. Детальные рекомендации мы предоставляем при записи.",
          "key": "public_faq.18.answer_ru.c6e12dad",
