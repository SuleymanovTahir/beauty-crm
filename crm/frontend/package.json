  "scripts": {
    "dev": "vite",
    "type-check": "tsc --noEmit",
    "build": "vite build",
    "build:seo": "PRERENDER=1 vite build",
    "lint": "tsc --noEmit --noUnusedLocals --noUnusedParameters",
    "check:boundary:backend": "cd ../backend && venv/bin/python scripts/diagnostics/verify_backend.py --boundaries --smoke-runtime",
    "check:boundary:frontend": "if rg -n \"(\\.{1,2}/)+site/frontend|/site/frontend|@beauty-crm/site\" src scripts > /dev/null; then echo '❌ CRM frontend contains cross-project site frontend import'; exit 1; else echo '✅ CRM frontend boundary check passed'; fi",
    "check:boundaries": "npm run check:boundary:backend && npm run check:boundary:frontend",
    "preview": "vite preview",
    "prerender:public": "npm run build:seo && node scripts/prerender-public.mjs",
    "postinstall": "patch-package || true",
    "i18n:setup": "node scripts/i18n/setup-i18n.js",
    "i18n:extract": "node ./node_modules/i18next-parser/bin/cli.js --config scripts/i18n/i18next-parser.config.js && node scripts/i18n/cleanup-duplicates.js > /dev/null 2>&1 && node scripts/i18n/fix-nested-keys.js > /dev/null 2>&1 && echo '✅ Extraction complete'",
    "i18n:check": "node scripts/check-translations.js",
    "i18n:check-only": "node scripts/check-translations.js",
    "i18n:sync": "node scripts/i18n-sync.js",
    "db:i18n:extract": "cd ../backend && venv/bin/python scripts/translations/db_extract.py",
    "db:i18n:translate": "cd ../backend && venv/bin/python scripts/translations/db_translate.py",
    "db:i18n:sync": "cd ../backend && venv/bin/python scripts/translations/db_sync.py",
    "db:i18n:sync-db": "cd ../backend && venv/bin/python scripts/translations/db_sync_to_db.py",
    "db:i18n:sync-frontend": "cd ../backend && venv/bin/python scripts/translations/sync_frontend_locales.py",
    "db:i18n:auto": "npm run i18n:extract && npm run db:i18n:extract && npm run db:i18n:translate && npm run db:i18n:sync && npm run db:i18n:sync-db && npm run db:i18n:sync-frontend && npm run frontend:i18n:auto && cd ../backend && venv/bin/python scripts/translations/fix_bad_translations.py",
    "i18n:static": "cd ../backend && venv/bin/python scripts/translations/static_translate.py",
    "frontend:i18n:auto": "cd ../backend && venv/bin/python scripts/translations/frontend_translate.py",
    "db:i18n:admin": "cd ../backend && venv/bin/python scripts/translations/translate_admin.py",
    "db:i18n:crm": "cd ../backend && venv/bin/python scripts/translations/translate_crm.py"
  },
  "overrides": {
    "glob": "^10.0.0",
    "rimraf": "^5.0.0"
  }
}
